{
  "type": "module",
  "name": "@essentx/elementaris-api-schema",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npx openapi-typescript ../../openapi_spec_v1.yaml -o ./dist/v1/index.d.ts",
    "release": "npx semantic-release"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./v1": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/v1/index.d.ts"
    }
  },
  "devDependencies": {
    "openapi-typescript": "6.7.4",
    "semantic-release": "23.0.2"
  },
  "keywords": [
    "essentx",
    "elementaris",
    "elementaris-api"
  ],
  "author": "Essentx AG <ops@essentx.com>",
  "contributors": [
    "Jan Biasi (https://github.com/janbiasi)"
  ],
  "license": "MIT"
}
